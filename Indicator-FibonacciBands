ATRperiod:=Input("ATR Period",1,500,14);
BANDperiod:=Input("EMA Period",1,500,14);
EMA:=MOV(c,BANDperiod,e);
R1:=ATR(ATRperiod);
R2:=ATR(ATRperiod)*1.618;
R3:=ATR(ATRperiod)*2.618;
R4:=ATR(ATRperiod)*4.236;
FIBOTOP4:=EMA+R4;
FIBOTOP3:=EMA+R3;
FIBOTOP2:=EMA+R2;
FIBOTOP1:=EMA+R1;
FIBOBOT1:=EMA-R1;
FIBOBOT2:=EMA-R2;
FIBOBOT3:=EMA-R3;
FIBOBOT4:=EMA-R4;
FIBOTOP4;
FIBOTOP3;
FIBOTOP2;
FIBOTOP1;
FIBOBOT1;
FIBOBOT2;
FIBOBOT3;
FIBOBOT4
